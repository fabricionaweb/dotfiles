#!/bin/sh

# Setup Pip (https://pypi.org/project/pip/) :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
sudo easy_install pip

# Pip stuff :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
echo "Installing pip stuff"
pip=(
  "doge"
)

for app in "${pip[@]}"; do
  echo "Installing $app"
  pip install $app
done